    "invest-nodejs-grpc-sdk": "git+https://github.com/mtvkand/invest-nodejs-grpc-sdk.git",
    "reflect-metadata": "^0.1.13",
    "typescript": "4.6.3",
    "winston": "^3.7.2",
    "winston-telegram": "^2.5.0"
  },
  "devDependencies": {
    "jest": "^28.1.1",
    "ts-node": "10.7.0",
    "tsconfig-replace-paths": "^0.0.11"
  }
}
