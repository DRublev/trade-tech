{
  "name": "shared-kernel",
  "main": "./index.ts",
  "version": "1.0.8",
  "private": true,
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.build.json && tsconfig-replace-paths -p tsconfig.build.json",
